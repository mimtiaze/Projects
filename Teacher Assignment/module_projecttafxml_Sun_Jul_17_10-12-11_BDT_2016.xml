    <pathelement location="${basedir}/../poi-3.14/xmlbeans-2.6.0.jar"/>
  </path>
  
  <path id="projecttafxml.runtime.module.classpath">
    <pathelement location="${projecttafxml.testoutput.dir}"/>
    <pathelement location="${projecttafxml.output.dir}"/>
    <pathelement location="${basedir}/../poi-3.14/commons-codec-1.10.jar"/>
    <pathelement location="${basedir}/../poi-3.14/commons-logging-1.2.jar"/>
    <pathelement location="${basedir}/../poi-3.14/curvesapi-1.03.jar"/>
    <pathelement location="${basedir}/../poi-3.14/junit-4.12.jar"/>
    <pathelement location="${basedir}/../poi-3.14/log4j-1.2.17.jar"/>
