  </path>
  
  
  <patternset id="excluded.from.module.projecttafxml">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.projecttafxml">
    <patternset refid="excluded.from.module.projecttafxml"/>
  </patternset>
  
  <path id="projecttafxml.module.sourcepath">
    <dirset dir="${module.projecttafxml.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.projecttafxml" depends="compile.module.projecttafxml.production,compile.module.projecttafxml.tests" description="Compile module ProjectTAFxml"/>
  
  <target name="compile.module.projecttafxml.production" depends="register.custom.compilers" description="Compile module ProjectTAFxml; production classes">
    <mkdir dir="${projecttafxml.output.dir}"/>
    <javac2 destdir="${projecttafxml.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.projecttafxml}/javac">
      <compilerarg line="${compiler.args.projecttafxml}"/>
      <bootclasspath refid="projecttafxml.module.bootclasspath"/>
      <classpath refid="projecttafxml.module.production.classpath"/>
      <src refid="projecttafxml.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.projecttafxml"/>
    </javac2>
    
    <copy todir="${projecttafxml.output.dir}">
      <fileset dir="${module.projecttafxml.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.projecttafxml.tests" depends="register.custom.compilers,compile.module.projecttafxml.production" description="compile module ProjectTAFxml; test classes" unless="skip.tests"/>
  
  <target name="clean.module.projecttafxml" description="cleanup module">
    <delete dir="${projecttafxml.output.dir}"/>
    <delete dir="${projecttafxml.testoutput.dir}"/>
  </target>
</project>