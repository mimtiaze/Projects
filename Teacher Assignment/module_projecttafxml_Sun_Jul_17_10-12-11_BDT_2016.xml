  <property name="compiler.args.projecttafxml" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="projecttafxml.output.dir" value="${module.projecttafxml.basedir}/out/production/ProjectTAFxml"/>
